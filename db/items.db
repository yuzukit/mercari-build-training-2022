DROP TABLE items;
DROP TABLE category;

CREATE TABLE items (
  id INTEGER PRIMARY KEY AUTOINCREMENT, 
  name TEXT, 
  category_id INTEGER,
  image TEXT,
  FOREIGN KEY (category_id) REFERENCES category(id)
  );

CREATE TABLE category (
  id INTEGER PRIMARY KEY,
  category TEXT
  );

